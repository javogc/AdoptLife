<%= form_for @event do |f| %>

  <%=f.label :date %>
  <%= date_field(:event,:date) %>

  <%= f.label :name %>
  <%=f.text_field :name %>

  <%=f.label :addrline1 %>
  <%=f.text_field :addrline1%>

  <%=f.label :addrline2 %>
  <%=f.text_field :addrline2%>

  <%=f.label :description %>
  <%=f.text_area :description%>
  <br>
  <br>

  <%=f.submit "Create Event" %>
<%end%>
